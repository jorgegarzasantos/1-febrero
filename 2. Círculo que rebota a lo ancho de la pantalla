//Jorge Eduardo Garza Santos #1656765 Persepcion N6
var x=200
var vel= 3
function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(127,181,181);
   fill(255,0,0)
  strokeWeight(5)
  stroke(0,0,255)
  ellipse(x,200,50,50)
  x=x+vel
  if(x>370){
    vel=vel*-1
  }
    if(x<30){
    vel=vel*-1
  }
} 
